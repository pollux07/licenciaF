{% extends '::base.html.twig' %}

{% block body -%}
    <h1>procedencia</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Fechallegada</th>
                <td>{{ entity.fechaLlegada|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Numeroseriefabricante</th>
                <td>{{ entity.numeroSerieFabricante }}</td>
            </tr>
            <tr>
                <th>Numeroseriewarriors</th>
                <td>{{ entity.numeroSerieWarriors }}</td>
            </tr>
            <tr>
                <th>Fechasalida</th>
                <td>{{ entity.fechaSalida|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('procedencia') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('procedencia_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
